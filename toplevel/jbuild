(jbuild_version 1)

(library (
  (name        top_runtime)
  (public_name probnetkat.top_runtime)
  (libraries  (probnetkat))
  (modules (top_runtime))
))

(executable
 ((name        top)
  (public_name probnetkat.top)
  (libraries  (utop probnetkat probnetkat.top_runtime))
  (link_flags (-linkall))
  (modules (top))
  (modes      (byte))))
